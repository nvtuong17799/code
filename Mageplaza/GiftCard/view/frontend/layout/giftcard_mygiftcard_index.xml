<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">My Gift Card</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Mageplaza\GiftCard\Block\Mygiftcard\Dashboard\Info" name="customer_giftcard_dashboard_info" template="Mageplaza_GiftCard::giftcard/dashboard/info.phtml" cacheable="false"/>
            <block class="Mageplaza\GiftCard\Block\Mygiftcard\Dashboard\History" name="customer_giftcard_dashboard_history" template="Mageplaza_GiftCard::giftcard/dashboard/history.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
